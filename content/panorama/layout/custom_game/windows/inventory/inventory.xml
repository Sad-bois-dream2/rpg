<root>
    <styles>
        <include src="file://{resources}/styles/windows/basic_window.css"/>
        <include src="file://{resources}/styles/windows/inventory_window.css"/>
    </styles>
    <scripts>
        <include src="file://{resources}/scripts/windows/basic_window.js"/>
        <include src="file://{resources}/scripts/windows/inventory.js"/>
    </scripts>
    <Panel class="Root" hittest="false">
        <Panel id="MainWindow" class="Window" style="width: 854px; height: 770px; visibility: collapse;">
            <Panel class="WindowContent">
                <Panel class="HeroContainer">
                    <Panel class="HeroItems">
                        <Panel id="HeroModelContainer"/>
                        <Panel id="LeftItemSlots" class="LeftItemSlots">
                            <Panel id="InventoryEquippedSlot0" class="InventoryEquippedSlot empty" draggable="true"
                                   oncontextmenu="OnRightClickOnInventoryEquippedSlot(0)"
                                   onmouseover="ShowEquippedItemTooltip(0)" onmouseout="HideItemTooltip()"
                                   showtooltip="false">
                                <DOTAItemImage showtooltip="false" hittest="false" id="InventoryEquippedSlotImage0"
                                               scaling="stretch-to-fit-x-preserve-aspect" itemname="item_claymore"/>
                                <Panel id="InventoryEquippedSlotBorder0" class="ItemBorder"/>
                                <Label text="#DOTA_Inventory_slot_mainhand"/>
                            </Panel>
                            <Panel id="InventoryEquippedSlot1" class="InventoryEquippedSlot empty" draggable="true"
                                   oncontextmenu="OnRightClickOnInventoryEquippedSlot(1)"
                                   onmouseover="ShowEquippedItemTooltip(1)" onmouseout="HideItemTooltip()"
                                   showtooltip="false">
                                <DOTAItemImage showtooltip="false" hittest="false" id="InventoryEquippedSlotImage1"
                                               scaling="stretch-to-fit-x-preserve-aspect" itemname="item_chainmail"/>
                                <Panel id="InventoryEquippedSlotBorder1" class="ItemBorder"/>
                                <Label text="#DOTA_Inventory_slot_body"/>
                            </Panel>
                            <Panel id="InventoryEquippedSlot2" class="InventoryEquippedSlot empty" draggable="true"
