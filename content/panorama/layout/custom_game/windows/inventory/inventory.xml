                                       src="file://{images}/custom_game/hud/stats/bat.png"/>
                                <Label class="WindowText InventoryAttributeLabel" text="#DOTA_Tooltip_BaseAttackTime"/>
                                <Label id="BaseAttackTimeLabel" class="WindowText InventoryAttributeValue" text="0"/>
                            </Panel>
                            <Panel id="Attack Damage" class="InventoryAttributeWithValue">
                                <Image class="StatIcon" src="file://{images}/custom_game/hud/stats/attack_damage.png"/>
                                <Label class="WindowText InventoryAttributeLabel" text="#DOTA_Tooltip_AttackDamage"/>
                                <Label id="AttackDamageLabel" class="WindowText InventoryAttributeValue" text="0"/>
                            </Panel>
                            <Panel id="MoveSpeed" class="InventoryAttributeWithValue">
                                <Image class="StatIcon" src="file://{images}/custom_game/hud/stats/move_speed.png"/>
                                <Label class="WindowText InventoryAttributeLabel" text="#DOTA_Tooltip_MoveSpeed"/>
                                <Label id="MoveSpeedLabel" class="WindowText InventoryAttributeValue" text="0"/>
                            </Panel>
                            <Panel id="ManaRegen" class="InventoryAttributeWithValue">
                                <Image class="StatIcon" src="file://{images}/custom_game/hud/stats/mana_regeneration.png"/>
                                <Label class="WindowText InventoryAttributeLabel" text="#DOTA_Tooltip_ManaRegen"/>
                                <Label id="ManaRegenLabel" class="WindowText InventoryAttributeValue" text="0"/>
                            </Panel>
