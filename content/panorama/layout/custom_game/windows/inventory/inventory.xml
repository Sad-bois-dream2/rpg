                                   onmouseover="ShowEquippedItemTooltip(6)" onmouseout="HideItemTooltip()"
                                   showtooltip="false">
                                <DOTAItemImage showtooltip="false" hittest="false" id="InventoryEquippedSlotImage6"
                                               scaling="stretch-to-fit-x-preserve-aspect" itemname="item_cloak"/>
                                <Panel id="InventoryEquippedSlotBorder6" class="ItemBorder"/>
                                <Label text="#DOTA_Inventory_slot_cape"/>
                            </Panel>
                            <Panel id="InventoryEquippedSlot7" class="InventoryEquippedSlot empty" draggable="true"
                                   oncontextmenu="OnRightClickOnInventoryEquippedSlot(7)"
                                   onmouseover="ShowEquippedItemTooltip(7)" onmouseout="HideItemTooltip()"
                                   showtooltip="false">
                                <DOTAItemImage showtooltip="false" hittest="false" id="InventoryEquippedSlotImage7"
                                               scaling="stretch-to-fit-x-preserve-aspect" itemname="item_shivas_guard"/>
                                <Panel id="InventoryEquippedSlotBorder7" class="ItemBorder"/>
                                <Label text="#DOTA_Inventory_slot_shoulder"/>
                            </Panel>
                            <Panel id="InventoryEquippedSlot8" class="InventoryEquippedSlot empty" draggable="true"
                                   oncontextmenu="OnRightClickOnInventoryEquippedSlot(8)"
                                   onmouseover="ShowEquippedItemTooltip(8)" onmouseout="HideItemTooltip()"
                                   showtooltip="false">
                                <DOTAItemImage showtooltip="false" hittest="false" id="InventoryEquippedSlotImage8"
                                               scaling="stretch-to-fit-x-preserve-aspect" itemname="item_gloves"/>
