                    </Panel>
                    <Panel class="HeroStats" style="padding-top: 30px;">
                        <Panel class="SwitchPageButtons">
                            <Panel id="Page0Button" class="WindowButton" onactivate="OnSwitchPageButtonClick(0)">
                                <Label text="I"/>
                            </Panel>
                            <Panel id="Page1Button" class="WindowButton" onactivate="OnSwitchPageButtonClick(1)">
                                <Label text="II"/>
                            </Panel>
                            <Panel id="Page2Button" class="WindowButton" onactivate="OnSwitchPageButtonClick(2)">
                                <Label text="III"/>
                            </Panel>
                        </Panel>
                        <Panel class="HeroNameContainer">
                            <DOTAHeroImage id="HeroIcon" class="HeroIcon" heroname="npc_dota_hero_crystal_maiden"
                                           heroimagestyle="icon"/>
                            <DOTAUserName id="HeroName" class="WindowText HeroName" steamid="local"/>
                        </Panel>
                        <Panel id="Page0" class="HeroStatsPage">
                            <Label class="InventoryAttributeGrouplabel WindowText" text="#DOTA_Tooltip_Attributes"/>
                            <Panel class="InventoryAttributeWithValue">
                                <Image class="StatIcon" src="s2r://panorama/images/primary_attribute_icons/primary_attribute_icon_strength_psd.vtex"/>
                                <Label class="WindowText InventoryAttributeLabel" text="#DOTA_Tooltip_Strength"/>
                                <Label id="StrengthLabel" class="WindowText InventoryAttributeValue" text="0"/>
                            </Panel>
                            <Panel class="InventoryAttributeWithValue">
                                <Image class="StatIcon" src="s2r://panorama/images/primary_attribute_icons/primary_attribute_icon_agility_psd.vtex"/>
                                <Label class="WindowText InventoryAttributeLabel" text="#DOTA_Tooltip_Agility"/>
                                <Label id="AgilityLabel" class="WindowText InventoryAttributeValue" text="0"/>
                            </Panel>
                            <Panel class="InventoryAttributeWithValue">
                                <Image class="StatIcon" src="s2r://panorama/images/primary_attribute_icons/primary_attribute_icon_intelligence_psd.vtex"/>
                                <Label class="WindowText InventoryAttributeLabel" text="#DOTA_Tooltip_Intelligence"/>
                                <Label id="IntelligenceLabel" class="WindowText InventoryAttributeValue" text="0"/>
                            </Panel>
                            <Label class="InventoryAttributeGrouplabel WindowText" text="#DOTA_Tooltip_GeneralStats"/>
                            <Panel class="InventoryAttributeWithValue">
                                <Image class="StatIcon" src="file://{images}/custom_game/hud/stats/health.png"/>
                                <Label class="WindowText InventoryAttributeLabel" text="#DOTA_Tooltip_Health"/>
                                <Label id="HealthLabel" class="WindowText InventoryAttributeValue" text="0"/>
                            </Panel>
                            <Panel id="Mana" class="InventoryAttributeWithValue">
                                <Image class="StatIcon" src="file://{images}/custom_game/hud/stats/mana.png"/>
                                <Label class="WindowText InventoryAttributeLabel" text="#DOTA_Tooltip_Mana"/>
                                <Label id="ManaLabel" class="WindowText InventoryAttributeValue" text="0"/>
                            </Panel>
                            <Panel id="CooldownReduction" class="InventoryAttributeWithValue">
                                <Image class="StatIcon" src="file://{images}/custom_game/hud/stats/cooldown_reduction.png"/>
                                <Label class="WindowText InventoryAttributeLabel"
                                       text="#DOTA_Tooltip_CooldownReduction"/>
                                <Label id="CooldownReductionLabel" class="WindowText InventoryAttributeValue"
                                       text="0%"/>
                            </Panel>
                            <Panel id="CurrentXP" class="InventoryAttributeWithValue">
                                <Image class="StatIcon" src="file://{images}/custom_game/hud/stats/exp.png"/>
                                <Label class="WindowText InventoryAttributeLabel" text="#DOTA_Tooltip_CurrentXP"/>
                                <Label id="CurrentXPLabel" class="WindowText InventoryAttributeValue" text="0/0"/>
                            </Panel>
                        </Panel>
                        <Panel id="Page1" class="HeroStatsPage">
                            <Label class="InventoryAttributeGrouplabel WindowText" text="#DOTA_Tooltip_OffensiveStats"/>
                            <Panel id="SpellDamage" class="InventoryAttributeWithValue"
                                   onmouseover="DOTAShowTitleTextTooltip('#DOTA_Tooltip_SpellDamage', '#DOTA_Tooltip_SpellDamage_Description')"
                                   onmouseout="DOTAHideTitleTextTooltip()">
                                <Image class="StatIcon" src="file://{images}/custom_game/hud/stats/spell_damage.png"/>
                                <Label class="WindowText InventoryAttributeLabel" text="#DOTA_Tooltip_SpellDamage"/>
                                <Label id="SpellDamageLabel" class="WindowText InventoryAttributeValue" text="0%"/>
                            </Panel>
                            <Panel id="Spellhaste" class="InventoryAttributeWithValue"
                                   onmouseover="DOTAShowTitleTextTooltip('#DOTA_Tooltip_SpellHaste', '#DOTA_Tooltip_SpellHaste_Description')"
                                   onmouseout="DOTAHideTitleTextTooltip()">>
                                <Image class="StatIcon" src="file://{images}/custom_game/hud/stats/cast_speed.png"/>
                                <Label class="WindowText InventoryAttributeLabel" text="#DOTA_Tooltip_SpellHaste"/>
                                <Label id="SpellHasteLabel" class="WindowText InventoryAttributeValue" text="0%"/>
                            </Panel>
                            <Panel id="AttackRange" class="InventoryAttributeWithValue">
