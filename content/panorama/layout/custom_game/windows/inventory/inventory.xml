                                   onmouseover="ShowEquippedItemTooltip(4)" onmouseout="HideItemTooltip()"
                                   showtooltip="false">
                                <DOTAItemImage showtooltip="false" hittest="false" id="InventoryEquippedSlotImage4"
                                               scaling="stretch-to-fit-x-preserve-aspect"
                                               itemname="item_helm_of_iron_will"/>
                                <Panel id="InventoryEquippedSlotBorder4" class="ItemBorder"/>
                                <Label text="#DOTA_Inventory_slot_helmet"/>
                            </Panel>
                        </Panel>

                        <Panel id="MiddleItemSlotsFromBottom" class="MiddleItemSlots FromBottom">
                            <Panel class="LeftRings">
                                <Panel id="InventoryEquippedSlot9" class="InventoryEquippedSlot empty" draggable="true"
                                       oncontextmenu="OnRightClickOnInventoryEquippedSlot(9)"
                                       onmouseover="ShowEquippedItemTooltip(9)" onmouseout="HideItemTooltip()"
                                       showtooltip="false">
                                    <DOTAItemImage showtooltip="false" hittest="false" id="InventoryEquippedSlotImage9"
                                                   scaling="stretch-to-fit-x-preserve-aspect"
                                                   itemname="item_ring_of_protection"/>
                                    <Panel id="InventoryEquippedSlotBorder9" class="ItemBorder"/>
                                    <Label text="#DOTA_Inventory_slot_ring"/>
                                </Panel>
                            </Panel>
                            <Panel id="InventoryEquippedSlot10" class="InventoryEquippedSlot empty" draggable="true"
                                   oncontextmenu="OnRightClickOnInventoryEquippedSlot(10)"
                                   onmouseover="ShowEquippedItemTooltip(10)" onmouseout="HideItemTooltip()"
                                   showtooltip="false">
                                <DOTAItemImage showtooltip="false" hittest="false" id="InventoryEquippedSlotImage10"
                                               scaling="stretch-to-fit-x-preserve-aspect"
                                               itemname="item_belt_of_strength"/>
                                <Panel id="InventoryEquippedSlotBorder10" class="ItemBorder"/>
                                <Label text="#DOTA_Inventory_slot_belt"/>
                            </Panel>
                            <Panel class="RightRings">
                                <Panel id="InventoryEquippedSlot11" class="InventoryEquippedSlot empty" draggable="true"
                                       oncontextmenu="OnRightClickOnInventoryEquippedSlot(11)"
