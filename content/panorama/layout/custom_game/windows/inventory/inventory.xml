                                   oncontextmenu="OnRightClickOnInventoryEquippedSlot(5)"
                                   onmouseover="ShowEquippedItemTooltip(5)" onmouseout="HideItemTooltip()"
                                   showtooltip="false">
                                <DOTAItemImage showtooltip="false" hittest="false" id="InventoryEquippedSlotImage5"
