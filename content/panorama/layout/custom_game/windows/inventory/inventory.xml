                                <Label class="WindowText InventoryAttributeLabel" text="#DOTA_Tooltip_HealthRegen"/>
                                <Label id="HealthRegenLabel" class="WindowText InventoryAttributeValue" text="0"/>
                            </Panel>
                            <Panel id="DamageReduction" class="InventoryAttributeWithValue"
                                   onmouseover="DOTAShowTitleTextTooltip('#DOTA_Tooltip_DamageReduction', '#DOTA_Tooltip_DamageReduction_Description')"
                                   onmouseout="DOTAHideTitleTextTooltip()">
                                <Image class="StatIcon" src="file://{images}/custom_game/hud/stats/damage_reduction.png"/>
                                <Label class="WindowText InventoryAttributeLabel" text="#DOTA_Tooltip_DamageReduction"/>
                                <Label id="DamageReductionLabel" class="WindowText InventoryAttributeValue" text="0%"/>
                            </Panel>
                            <Panel id="AggroCaused" class="InventoryAttributeWithValue"
                                   onmouseover="DOTAShowTitleTextTooltip('#DOTA_Tooltip_AggroCaused', '#DOTA_Tooltip_AggroCaused_Description')"
                                   onmouseout="DOTAHideTitleTextTooltip()">
                                <Image class="StatIcon" src="file://{images}/custom_game/hud/stats/threat.png"/>
                                <Label class="WindowText InventoryAttributeLabel" text="#DOTA_Tooltip_AggroCaused"/>
                                <Label id="AggroCausedLabel" class="WindowText InventoryAttributeValue" text="100%"/>
                            </Panel>
                            <Panel id="DebuffResistance" class="InventoryAttributeWithValue"
                                   onmouseover="DOTAShowTitleTextTooltip('#DOTA_Tooltip_DebuffResistance', '#DOTA_Tooltip_DebuffResistance_Description')"
                                   onmouseout="DOTAHideTitleTextTooltip()">
                                <Image class="StatIcon" src="file://{images}/custom_game/hud/stats/status_resistance.png"/>
                                <Label class="WindowText InventoryAttributeLabel"
                                       text="#DOTA_Tooltip_DebuffResistance"/>
                                <Label id="DebuffResistanceLabel" class="WindowText InventoryAttributeValue" text="0%"/>
                            </Panel>
                            <Panel id="HealingReceived" class="InventoryAttributeWithValue"
                                   onmouseover="DOTAShowTitleTextTooltip('#DOTA_Tooltip_HealingReceived', '#DOTA_Tooltip_HealingReceived_Description')"
                                   onmouseout="DOTAHideTitleTextTooltip()">
                                <Image class="StatIcon" src="file://{images}/custom_game/hud/stats/healing_received.png"/>
                                <Label class="WindowText InventoryAttributeLabel" text="#DOTA_Tooltip_HealingReceived"/>
                                <Label id="HealingReceivedLabel" class="WindowText InventoryAttributeValue"
                                       text="100%"/>
                            </Panel>
                            <Panel id="HealingCaused" class="InventoryAttributeWithValue"
                                   onmouseover="DOTAShowTitleTextTooltip('#DOTA_Tooltip_HealingCaused', '#DOTA_Tooltip_HealingCaused_Description')"
                                   onmouseout="DOTAHideTitleTextTooltip()">
                                <Image class="StatIcon" src="file://{images}/custom_game/hud/stats/healing_caused.png"/>
                                <Label class="WindowText InventoryAttributeLabel" text="#DOTA_Tooltip_HealingCaused"/>
                                <Label id="HealingCausedLabel" class="WindowText InventoryAttributeValue" text="100%"/>
                            </Panel>
                            <Panel id="DefensiveElements" class="BoxedAttributes"
                                   onmouseover="DOTAShowTitleTextTooltip('#DOTA_Tooltip_ElementsProtection', '#DOTA_Tooltip_ElementsProtection_Description')"
                                   onmouseout="DOTAHideTitleTextTooltip()"/>
                        </Panel>
                    </Panel>
                </Panel>
                <Panel id="InventoryContainer" class="InventoryContainer"/>
            </Panel>
            <TextButton class="WindowCloseIcon" text="X" onactivate="CloseWindow()" />
        </Panel>
        <!-- weird hack to make them auto recompile -->
        <Panel style="visibility: collapse;">
            <Image src="file://{images}/custom_game/hud/stats/fire_element.png"/>
