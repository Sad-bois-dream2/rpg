                                   showtooltip="false">
                                <DOTAItemImage showtooltip="false" hittest="false" id="InventoryEquippedSlotImage2"
                                               scaling="stretch-to-fit-x-preserve-aspect"
                                               itemname="item_inventory_legs_background"/>
                                <Panel id="InventoryEquippedSlotBorder2" class="ItemBorder"/>
                                <Label text="#DOTA_Inventory_slot_legs"/>
                            </Panel>
                            <Panel id="InventoryEquippedSlot3" class="InventoryEquippedSlot empty" draggable="true"
                                   oncontextmenu="OnRightClickOnInventoryEquippedSlot(3)"
                                   onmouseover="ShowEquippedItemTooltip(3)" onmouseout="HideItemTooltip()"
                                   showtooltip="false">
                                <DOTAItemImage showtooltip="false" hittest="false" id="InventoryEquippedSlotImage3"
                                               scaling="stretch-to-fit-x-preserve-aspect" itemname="item_boots"/>
                                <Panel id="InventoryEquippedSlotBorder3" class="ItemBorder"/>
                                <Label text="#DOTA_Inventory_slot_boots"/>
                            </Panel>
                        </Panel>

                        <Panel id="RightItemSlots" class="RightItemSlots">
