                                <Label class="WindowText InventoryAttributeLabel" text="#DOTA_Tooltip_CriticalChance"/>
                                <Label id="CriticalChanceLabel" class="WindowText InventoryAttributeValue" text="0%"/>
                            </Panel>
                            <Panel id="OffensiveElements" class="BoxedAttributes"
                                   onmouseover="DOTAShowTitleTextTooltip('#DOTA_Tooltip_ElementsDamage', '#DOTA_Tooltip_ElementsDamage_Description')"
                                   onmouseout="DOTAHideTitleTextTooltip()"/>
                        </Panel>
                        <Panel id="Page2" class="HeroStatsPage">
                            <Label class="InventoryAttributeGrouplabel WindowText" text="#DOTA_Tooltip_DefensiveStats"/>
